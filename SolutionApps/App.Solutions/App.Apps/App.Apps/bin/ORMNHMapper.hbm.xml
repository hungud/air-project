<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="App.Models" namespace="App.Models">

  <!--Models class name="App.Models.Employee"-->
  <class name="App.Models.Employee" table="Employee" lazy="true">
    <id name="EmpId"></id>
    <property name="EmpName" />
    <property name="EmpSalary" />
  </class>
  <!--Models class name="App.Models.Employee"-->


  <!--Models class name="App.Models.EmpDetail"-->
  <class name="App.Models.EmpDetail" table="EmpDetail" lazy="true"  >
    <id name="FkEmpId"></id>
    <property name="EmpAddress" />
    <property name="EmpMobileNo" />
  </class>
  <!--Models class name="App.Models.EmpDetail"-->

</hibernate-mapping>